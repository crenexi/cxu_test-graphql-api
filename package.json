  "main": "dist/bin/www.js",
  "scripts": {
    "clean": "rm -rf ./dist ./logs",
    "lint": "gulp lint",
    "prestart": "npm run clean && npm run lint",
    "start": "./bin/start.sh",
    "tsnd": "tsnd --debounce 100 --no-notify --respawn --transpileOnly ./src/bin/www.ts",
    "prebuild": "npm run clean && npm run lint && npm run test",
    "build": "tsc -b",
    "test": "echo \"Error: no test specified\"",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli.js -f ./src/config/orm.config.ts",
    "orm:schema:log": "npm run typeorm:cli -- schema:log",
    "orm:schema:sync": "npm run typeorm:cli -- schema:sync",
    "orm:migration:show": "npm run typeorm:cli -- migration:show",
    "orm:migration:run": "npm run typeorm:cli -- migration:run",
    "orm:migration:revert": "npm run typeorm:cli -- migration:revert",
    "pm2:start": "pm2 start ./dist/config/pm2.config.js",
    "pm2:restart": "pm2 restart ./dist/config/pm2.config.js",
    "pm2:stop": "pm2 stop ./dist/config/pm2.config.js",
    "pm2:delete": "pm2 delete ./dist/config/pm2.config.js",
    "pm2:logs": "pm2 logs ./dist/config/pm2.config.js",
    "prerelease": "npm run orm:schema:log && npm run orm:migration:show",
    "release:start": "npm run prerelease & ./bin/release-start.sh",
    "release:finish": "./bin/release-finish.sh"
  },
